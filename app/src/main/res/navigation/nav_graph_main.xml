<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_main"
    app:startDestination="@id/easyMapsActivity">

    <fragment
        android:id="@+id/easyMapsActivity"
        android:name="com.guesswho.movetracker.ui.Mapfragment"
        android:label="EasyMapsActivity">
        <action
            android:id="@+id/action_easyMapsActivity_to_historyViewerActivty"
            app:destination="@id/historyViewerActivty" />
    </fragment>
    <fragment
        android:id="@+id/historyViewerActivty"
        android:name="com.guesswho.movetracker.ui.HistoryViewerFragment"
        android:label="fragment_history"
        tools:layout="@layout/fragment_history">
        <action
            android:id="@+id/action_historyViewerActivty_to_historyFragment3"
            app:destination="@id/historyFragment"
            app:enterAnim="@anim/fragment_close_enter"
            app:exitAnim="@anim/fragment_close_exit"
            app:popEnterAnim="@anim/fragment_open_enter"
            app:popExitAnim="@anim/fragment_open_exit" />
    </fragment>
    <fragment
        android:id="@+id/historyFragment"
        android:name="com.guesswho.movetracker.ui.HistoryFragment"
        android:label="HistoryFragment">
        <argument
            android:name="sessionId"
            android:defaultValue='""'
            app:argType="string" />
    </fragment>
</navigation>